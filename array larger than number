    <title>array_larger_than_number</title>
    <script lang="Javascript">
        function BiggerElements(val) {
            return function Largest(evalue) {
                return (evalue <= val);
            };
        }
        var result = [11, 45, 4, 31, 64, 10].filter(BiggerElements(30));
        document.writeln(result);
    </script>
