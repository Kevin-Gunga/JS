        function largestElements() {
            var elements = [15, 30, 100, 40, 60, 70];
            var largest = 0;
            for (i = 0; i < elements.length; i++) {
                if (elements[i] > largest) {
                    largest = elements[i];
                }
            }
            document.write(largest);

        }
        largestElements();
    </script>

